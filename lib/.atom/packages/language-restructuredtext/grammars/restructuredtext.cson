            'name': 'entity.name.directive.restructuredtext'
        'end': '^(?!\\1[ \\t])(?=\\S+)'
        'contentName': 'source.json'
        'patterns': [
          {
            'include': 'source.json'
          }
        ]
      }
      {
        'begin': '^([ \\t]*)\\.\\.\\scode-block::\\s(css)\\s*$'
        'captures':
          '2':
            'name': 'entity.name.directive.restructuredtext'
        'end': '^(?!\\1[ \\t])(?=\\S+)'
        'contentName': 'source.css'
        'patterns': [
